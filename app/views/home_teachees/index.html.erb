<h1>Here are the home teachees for you: </h1>
<div class='row'>
  <% cache do %>
    <% @home_teachees.each do |ht| %>
      <div class='col-md-3'>
        <div class='ht_index_box'>
          <div class='panel panel-default'>
            <div class='panel-heading'>
              Name: <%= link_to ht.name, home_teachee_path(ht) %>
            </div>
            <div class='panel-body'>
              <p>Address: <%= ht.street_number %></p>
              <p>Phone: <%= ht.phone1 %></p>
              <div class='issue_count'>
                <%= link_to ht.number_of_issues, home_teachee_path(ht) %>
              </div>
            </div>
          </div>
        </div>
      </div>
    <% end %>
  <% end %>
</div>
